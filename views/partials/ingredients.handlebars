<div class="container-fluid customList">
  <div class="row">
    <div class="col-6 customList">
      <div class="row">
        <div class="col-3 customList">
          <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
            <a class="nav-link active" id="v-pills-meat-tab" data-toggle="pill" href="#v-pills-meat" role="tab" aria-controls="v-pills-meat" aria-selected="true"><h2>Meat</h2></a>

            <a class="nav-link" id="v-pills-veggies-tab" data-toggle="pill" href="#v-pills-veggies" role="tab" aria-controls="v-pills-veggies" aria-selected="false"><h2>Veggies</h2></a>

            <a class="nav-link" id="v-pills-cheese-tab" data-toggle="pill" href="#v-pills-cheese" role="tab" aria-controls="v-pills-cheese" aria-selected="false"><h2>Cheese</h2></a>

            <a class="nav-link" id="v-pills-dressing-tab" data-toggle="pill" href="#v-pills-dressing" role="tab" aria-controls="v-pills-dressing" aria-selected="false"><h2>Dressing</h2></a>
          </div>
        </div>

        <div class="col-9 customList">
          <div class="tab-content" id="v-pills-tabContent">
            <div class="tab-pane fade show active" id="v-pills-meat" role="tabpanel" aria-labelledby="v-pills-meat-tab">
              {{#each meat}}   
                {{> checkbox}}
              {{/each}}
            </div>

            <div class="tab-pane fade" id="v-pills-veggies" role="tabpanel" aria-labelledby="v-pills-veggies-tab">
              {{#each veggies}}   
                {{> checkbox}}
              {{/each}}
            </div>

            <div class="tab-pane fade" id="v-pills-cheese" role="tabpanel" aria-labelledby="v-pills-cheese-tab">
              {{#each cheese}}
                {{> checkbox}}
              {{/each}}
            </div>

            <div class="tab-pane fade" id="v-pills-dressing" role="tabpanel" aria-labelledby="v-pills-dressing-tab">
              {{#each dressing}}
                {{> checkbox}}
              {{/each}}
            </div>
          </div>
        </div>
      </div>
      {{> submitform}}
    </div>
    
    <div id="itemsIMG" class="col-6 customList">
      <img id="topbun" class="itemIMG" src="./img/topbun.png">
      {{!-- <img class="itemIMG" src="./img/veggies-onion.png"> --}}
      <img id="botbun" class="itemIMG" src="./img/botbun.png">
    </div>
  </div>
</div>